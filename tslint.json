{
  "rulesDirectory": ["tslint-plugin-prettier"],
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rules": {
    "prettier": true,
    "jsx-no-lambda": false,
    "curly": [
      true,
      "ignore-same-line"
    ],
    "interface-name": false,
    "member-access": [
      true,
      "no-public"
    ],
    "triple-equals": false,
    "max-classes-per-file": [false],
    "object-literal-sort-keys": false
  }
}